      "source": [
        "# NumPy - Clipping Array Values\n",
        "import numpy as np\n",
        "\n",
        "# Creating an array\n",
        "array = np.array([1, 2, 3, 4, 5])\n",
        "\n",
        "# Clipping values to be between 2 and 4\n",
        "clipped_array = np.clip(array, 2, 4)\n",
        "\n",
        "print('Original Array:', array)\n",
        "print('Clipped Array:', clipped_array)"
      ],
      "metadata": {
