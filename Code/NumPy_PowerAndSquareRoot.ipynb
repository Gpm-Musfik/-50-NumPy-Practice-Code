        "# NumPy - Power and Square Root\n",
        "import numpy as np\n",
        "\n",
        "# Creating an array\n",
        "array = np.array([1, 4, 9, 16])\n",
        "\n",
        "# Power and square root\n",
        "squared = np.power(array, 2)\n",
        "sqrt = np.sqrt(array)\n",
        "\n",
        "print('Squared Array:', squared)\n",
        "print('Square Root of Array:', sqrt)"
      ],
