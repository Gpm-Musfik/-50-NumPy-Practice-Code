        "# NumPy - Modifying Array Values using Indexing\n",
        "import numpy as np\n",
        "\n",
        "# Creating an array\n",
        "array = np.array([10, 20, 30, 40, 50])\n",
        "\n",
        "# Modifying values at specific indices\n",
        "array[[1, 3]] = [100, 200]\n",
        "\n",
        "print('Modified Array:', array)"
