      "source": [
        "# NumPy - Indexing with np.take\n",
        "import numpy as np\n",
        "\n",
        "# Creating an array\n",
        "array = np.array([10, 20, 30, 40, 50])\n",
        "\n",
        "# Using np.take to select elements at specific indices\n",
        "indices = [0, 2, 4]\n",
        "result = np.take(array, indices)\n",
        "\n",
        "print('Selected Elements using np.take:', result)"
      ],
