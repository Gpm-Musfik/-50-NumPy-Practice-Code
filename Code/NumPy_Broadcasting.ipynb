        "# NumPy - Broadcasting\n",
        "import numpy as np\n",
        "\n",
        "# Creating an array\n",
        "array = np.array([1, 2, 3])\n",
        "\n",
        "# Broadcasting: Adding a scalar to an array\n",
        "result = array + 10\n",
        "\n",
        "print('Original Array:', array)\n",
        "print('Array after Broadcasting:', result)"
